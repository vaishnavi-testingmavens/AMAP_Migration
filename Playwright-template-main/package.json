  "description": "",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf allure-report/ rimraf allure-results/ && rimraf test-results",
    "execute:stage": "cross-env ENV=stage npx playwright test --config=playwright.config.ts",
    "execute:smoke:stage": "cross-env ENV=stage npx playwright test --grep @smoke --config=playwright.config.ts",
    "test:reporter": "allure generate allure-results -o allure-report --clean && allure open ./allure-report"
  },
